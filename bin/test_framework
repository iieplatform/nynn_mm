#!/bin/bash
log2_cli_num_min=$1
log2_cli_num_max=$(($2-1))
ip=$3
port_range_min=$4
port_range_max=$5
loop=$((2**${6}))
for i in `eval "echo {${log2_cli_num_min}..${log2_cli_num_max}}"`;do
	echo "----------log2_cli_num=$i------------"
	echo 
	./concurrency_cli_framework ${ip} ${port_range_min} ${port_range_max} ${loop} $i
	./util1 output.$i.*>>output
	echo "-------------------------------------"
	echo 
done 
wait
./quadtuples output
rm output* -fr
echo DONE
