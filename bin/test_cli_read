#!/bin/bash
ip=$1
port_range_min=$2
port_range_max=$3
switcher_num=$(($3-$2))
vbegin=$4
vend=$5
loop=$6
log2_client_num_min=$7
log2_client_num_max=$8
for i in `eval "echo {${log2_client_num_min}..$((${log2_client_num_max}-1))}"`;do
	./cli_read ${ip} $((${port_range_min}+$i%${switcher_num})) ${vbegin} ${vend} pop ${loop} &
done
